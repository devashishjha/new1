{
  "apphosting": {
    "backendId": "newlokality",
    "rootDir": "/",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "functions"
    ],
    "alwaysDeployFromSource": true
  },
  "hosting": {
    "site": "lokal-reels",
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "newlokality",
          "region": "asia-east1"
        }
      }
    ]
  }
}
